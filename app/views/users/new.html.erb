<% provide(:title, 'Sign up') %>
<% provide(:button_text, 'Create my account') %>
<h1>Sign up</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
      <%= render 'form', f: f %>  <!-- Передача переменной f в частичное представление -->
      <%= f.submit yield(:button_text), class: "btn btn-primary" %>  <!-- Использование переданного текста кнопки -->
    <% end %>
  </div>
</div>
